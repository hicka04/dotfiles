[color]
        ui = on
[alias]
        co = checkout
        st = status
        br = branch
        cmm = commit -m
        cma = commit --amend
        cmf = commit --fixup
        unstage = reset HEAD
        get = !ghq get
        sw = switch
	delete-merged-branch = !git branch --merged | egrep -v '\\*|main|master' | xargs git branch -d
[core]
        autocrlf = input
        quotepath = false
[init]
        defaultBranch = main
[push]
	autoSetupRemote = true
[rebase]
        autostash = true
        abbreviateCommands = true
